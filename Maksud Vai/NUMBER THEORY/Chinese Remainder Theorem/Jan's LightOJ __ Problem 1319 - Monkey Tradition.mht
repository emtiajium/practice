	border: 1px solid #DBDBDB;
	border-radius: 5;
}
.bbcode_quote_body {
	background-color: #EFEED3;
	padding: 5;
	border: 1px solid #DBDBDB;
	border-radius: 5;
}

.bbeditor-button{
	display:block;
	float:left;
	padding: 5 12 5 12;
	line-height:0;
	margin:1px;
	margin-right:0px;
