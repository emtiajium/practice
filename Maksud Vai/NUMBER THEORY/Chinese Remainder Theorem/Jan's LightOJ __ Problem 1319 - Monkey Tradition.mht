	width: 350px;
	padding: 0;
	margin: 0;
}

#mytable2 {
	width: 130px;
	padding: 0;
	margin: 0;
}

#mytable3 {
	width: 100%;
	padding: 5 5 5 5;
	margin: 0;
}

caption {
	padding: 0 0 5px 0;
	width: 700px;
	font: italic 11px "Trebuchet MS", Verdana, Arial, Helvetica, sans-serif;
