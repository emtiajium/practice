td.verdictbar {
	background: #fff;
	padding: 5px 5px 0px 8px;
	font: 10px "Trebuchet MS", Verdana, Arial, Helvetica, sans-serif;
	text-align: center;
}

.verdictcont {
	width: 25px;
	height: 120px;
	border: 1px solid #eeeeee;
	display: block;
}

.percentcont {
	width: 120px;
	height: 14px;
	border: 1px solid #99CC99;
	display: block;
	background-color: #fcfcfc;
}

.perfill {
	background: #aaff99 url(../images/stat_header.png) repeat-x 50% 50%;
	height: 14px;
}

.pertext {
	position: relative;
	top: -15px;
	text-align: center;
	font-weight: bold;
}

div#wrapper {
	float:left;
	width:100%;
}
div#content {
	min-height: 360px;
	margin:0 3px 0 200px;
	padding-bottom:4em;
	padding-left:1em;
	padding-right:1em;
	padding-top:1em;
	border-left:2px solid #002E55;
	border-right:2px solid #002E55;
	border-top:2px solid #002E55;
	border-bottom:2px solid #002E55;
	background: #E6EAE9;
	color: #4f6b72;
	overflow-x: auto;
}
div#problem {
	min-height: 280px;
	margin:3px 3px 3px 3px;
	padding-bottom:2em;
	padding-left:1em;
	padding-right:1em;
	padding-top:2em;
	border-left:2px solid #002E55;
	border-right:2px solid #002E55;
	border-top:2px solid #002E55;
	border-bottom:2px solid #002E55;
	background: white;
	color: black;
	text-decoration: none;
	overflow-x: auto;
}
div#problem_name {
	color: green;
	padding-bottom:5pt;
	font-size:14pt;
	font-family:"Cambria","serif";
	font-weight:bold;
	text-align:center;
}
div#problem_setter {
	font: bold 11px "Trebuchet MS", Verdana, Arial, Helvetica, sans-serif;
	color: #1f551f;
	letter-spacing: 2px;
	text-transform: uppercase;
	text-align: left;
}
div#navigation {
	float:left;
	width:200px;
	margin-left:-100%;
	margin-top: 2px;
	color:#fff;
}

.navcontainer {
	position: fixed;
	width:198px;
	border-right:1px solid #000;
	padding:0;
	margin-bottom:1em;
	background-color:transparent;
	text-align:center;
}
.navcontainer ul{
	list-style:none;
	margin:0;
	padding:0;
	border:none;
	width:198px;
	min-width:198px;
}
.navcontainer li {
	margin:0;
	font: bold 11px "Trebuchet MS", Verdana, Arial, Helvetica, sans-serif;
}

.navcontainer li a,.navcontainer li a:visited {
	display:block;
	padding:5px 5px 5px 5px;
	border-left:10px solid #000;
	border-right:10px solid #000;
	background-color: #002E55;
	text-decoration:none;
	font-weight:bold;
	width:168px;
	border-bottom:1px solid #1C85CA;
	color: #7BD0E0;
}

.navcontainer li#active a, .navcontainer li a:hover {
	border-left:10px solid #CCE8EB;
	border-right:10px solid #CCE8EB;
	background-color:#CCE8EB;
	color: #4f6b72;
}

a.submenu:link, a.submenu:visited, .navcontainer a.submenu:hover, .navcontainer li#active a.submenu {
	width: 150;
	border-left: 20px solid black;
	border-right: 20px solid black;
	background-color: transparent;
	font-size: 10;
}

.navcontainer li#active a.submenu {
	color: #c75f3e;
}

.navcontainer li#active a.submenu:hover {
	color: #4f6b72;
}

.navcontainer li.menutitle {
	font-size: 12px;
	border-left:10px solid #156396;
	border-right:10px solid #156396;
	border-top:10px solid #156396;
	text-align:center;
	color: #7BD0E0;
	padding:5px 5px 5px 5px;
	text-transform:uppercase;
	border-bottom:1px solid #1C85CA;
	background-color: #000;
}
.navcontainer li.menubottom {
	border-left:10px solid #156396;
	border-right:10px solid #156396;
	border-bottom:10px solid #156396;
	background-color: #000;
}

.buttons {
	text-align: center;
